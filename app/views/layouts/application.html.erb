<!DOCTYPE html>
<html>
  <head>
    <title>Lab1</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-dark bg-faded" style="background-color: #d9534f; margin-bottom: 20px">
      <%= link_to "CoderPhoto", root_path, class: 'navbar-brand' %>
      <ul class="nav navbar-nav">
        <li class="nav-item <%= 'active' if current_page?(photos_path) %>">
          <%= link_to "Timeline", photos_path, class: 'navbar-link' %>
        </li>
      </ul>
    </nav>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
